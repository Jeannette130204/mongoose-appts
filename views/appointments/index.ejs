<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h1>Appointment System</h1>
    <table>
        <thead>
            <tr>
                <th>Location</th>
                <th>Doctor</th>
                <th>Date</th>
                <th>Time</th>
            </tr>
        </thead>
            <tbody>
                <% appointments.forEach(function(a) { %>
                    <tr>
                        <td>
                            <%= a.location %>
                        </td>
                        <td>
                            <%= a.doctor %>
                        </td>
                        <td>
                            <%= a.date %>
                        </td>
                        <td>
                            <%= a.time %>
                        </td>
                    </tr>
                    <% }); %>
            </tbody>
    </table>
</body>

</html>
<a href="/appointments/new">new</a>